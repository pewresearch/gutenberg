{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "core/tabs",
	"title": "Tabs",
	"version": "1.0.0",
	"category": "design",
	"allowedBlocks": [ "core/tab" ],
	"attributes": {
		"orientation": {
			"type": "string",
			"default": "horizontal",
			"enum": [ "horizontal", "vertical" ]
		},
		"tabBackgroundColor": {
			"type": "string"
		},
		"customTabBackgroundColor": {
			"type": "string"
		},
		"tabHoverColor": {
			"type": "string"
		},
		"customTabHoverColor": {
			"type": "string"
		},
		"tabActiveColor": {
			"type": "string"
		},
		"customTabActiveColor": {
			"type": "string"
		},
		"tabTextColor": {
			"type": "string"
		},
		"customTabTextColor": {
			"type": "string"
		},
		"tabActiveTextColor": {
			"type": "string"
		},
		"customTabActiveTextColor": {
			"type": "string"
		},
		"tabHoverTextColor": {
			"type": "string"
		},
		"customTabHoverTextColor": {
			"type": "string"
		}
	},
	"example": {
		"attributes": {},
		"innerBlocks": [
			{
				"name": "core/tab",
				"attributes": {
					"label": "Tab 1",
					"slug": "tab-1",
					"tabIndex": 0
				},
				"innerBlocks": [
					{
						"name": "core/paragraph",
						"attributes": {
							"content": "Tab 1 content"
						}
					}
				]
			},
			{
				"name": "core/tab",
				"attributes": {
					"label": "Tab 2",
					"slug": "tab-2",
					"tabIndex": 1
				}
			},
			{
				"name": "core/tab",
				"attributes": {
					"label": "Tab 3",
					"slug": "tab-3",
					"tabIndex": 2
				}
			}
		]
	},
	"styles": [
		{
			"name": "tabbed",
			"label": "Tabbed",
			"isDefault": true
		},
		{
			"name": "tabbed-outline",
			"label": "Tabbed Outline"
		},
		{
			"name": "underlined",
			"label": "Underlined"
		},
		{
			"name": "pills",
			"label": "Pills"
		},
		{
			"name": "underlined-pills",
			"label": "Underlined Pills"
		}
	],
	"supports": {
		"align": true,
		"color": {
			"text": false,
			"background": false
		},
		"html": false,
		"interactivity": true,
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"fontFamily": true
		}
	},
	"textdomain": "tabs",
	"style": "wp-block-tabs",
	"editorStyle": "wp-block-tabs-editor"
}
